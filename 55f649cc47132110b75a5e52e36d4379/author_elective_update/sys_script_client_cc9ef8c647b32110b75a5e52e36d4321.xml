        <messages/>
        <name>Populate Universoty Name</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

   //Type appropriate comment here, and begin script below
	//alert("Minal - Display message");
	var getUniName = new GlideAjax('GetUniversityName');
	getUniName.addParam('sysparm_name', 'getUniversityName');
	getUniName.addParam('sysparm_userID', g_form.getValue('u_reference_1'));
	//alert(g_form.getValue('u_reference_1'));
	getUniName.getXML(populateUniName);
   
	function populateUniName(response) {
		var uniName = response.responseXML.documentElement.getAttribute("answer");
		g_form.clearValue('u_string_4');
		g_form.setValue('u_string_4', uniName);
	}
}]]></script>
